sh /etc/init.d/rc.mc_defaults

set MIXER FMU_octo_cox

set PWM_OUTPUTS 12345678

if [ $DO_AUTOCONFIG == yes ]
then
# TODO REVIEW
  param set MC_ROLL_P 3.8
  param set MC_ROLLRATE_P 0.09
  param set MC_ROLLRATE_I 0.0
  param set MC_ROLLRATE_D 0.0031
  param set MC_PITCH_P 3.8
  param set MC_PITCHRATE_P 0.09
  param set MC_PITCHRATE_I 0.0
  param set MC_PITCHRATE_D 0.0031
  param set MC_YAW_P 1.0
  param set MC_YAWRATE_P 0.25
  param set MC_YAWRATE_I 0.0
  param set MC_YAWRATE_D 0.002
fi
